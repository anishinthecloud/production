    <validationRules>
        <fullName>Closed_record_must_link_to_release</fullName>
        <active>false</active>
        <description>Work Requests must be linked to a release record before changing the status to close - completed.</description>
        <errorConditionFormula>ISPICKVAL( Stage__c , &quot;Closed - completed&quot;)&amp;&amp;
Release_records__c &lt; 1</errorConditionFormula>
        <errorMessage>You must link this record to a Release before changing the status to &apos;Closed - completed&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Work_Requests_In_development_Approved_by</fullName>
        <active>true</active>
        <description>If work request stage is &apos;In Development&apos; the request must be approved</description>
        <errorConditionFormula>ISPICKVAL(Stage__c, &quot;In development&quot;) &amp;&amp;  Approved_By__c = &quot;&quot; &amp;&amp; ISPICKVAL(Category__c, &quot;Change Request&quot;)</errorConditionFormula>
        <errorDisplayField>Approved_By__c</errorDisplayField>
        <errorMessage>All Change Requests must be approved before changing the stage to &apos;In development&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Work_Requests_In_development_due_date</fullName>
        <active>true</active>
        <description>If work request stage is &apos;In Development&apos; the request must have a due date</description>
        <errorConditionFormula>ISPICKVAL(Stage__c, &quot;In development&quot;) &amp;&amp; ISBLANK(Due_Date__c)</errorConditionFormula>
        <errorDisplayField>Due_Date__c</errorDisplayField>
        <errorMessage>All requests must have a due date before changing the stage to &apos;In development&apos;</errorMessage>
    </validationRules>
</CustomObject>
