    <validationRules>
        <fullName>Call_limit_cannot_be_zero</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($Setup.No_Validations__c.Flag__c) &amp;&amp;
(New_Call_Limit__c  &lt;= 0)</errorConditionFormula>
        <errorDisplayField>Billing_Account__c</errorDisplayField>
        <errorMessage>Call limit cannot be zero or negative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>First_Installment_Date_Within_X_days</fullName>
        <active>true</active>
        <description>The first instalment date is over acceptable tolerance.</description>
        <errorConditionFormula>NOT($Setup.No_Validations__c.Flag__c) &amp;&amp; (
 IF(One_off_Payment__c &gt; 0, Date_of_First_Installment__c &gt; TODAY() + $Setup.Collections_Settings__c.Payment_Days_from_Now_Tolerance_Oneoff__c, Date_of_First_Installment__c &gt; TODAY() + $Setup.Collections_Settings__c.Payment_Days_from_Now_Tolerance__c ) 
)</errorConditionFormula>
        <errorMessage>The first instalment date has to be within the specified amount of  days from now.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Phone_Number_if_Provided</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT( ISBLANK( Preferred_Contact_Number__c )), NOT(REGEX( Preferred_Contact_Number__c, &quot;\\+?[\\d\\-\\s]+&quot;)))</errorConditionFormula>
        <errorMessage>The Preferred Phone Number you have provided does not have the required format. Only numbers, - and spaces are allowed, and the number may start with + or not.</errorMessage>
    </validationRules>
